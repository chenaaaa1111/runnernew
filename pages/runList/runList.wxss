.searchbg{
  height: 100%;
  width: 100%;
  background-image: url("http://47.101.136.23:8080/sport/image/698308119322920474.png");
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}
.search-box {
  position: relative;
  z-index: 20;
  height: 88rpx;
  background-color: white;
  border: 1px solid #999;
  border-radius: 4px;
  margin: 20rpx 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.search-box input {
  margin-left: 42rpx; 
}
.search-box icon {
  margin-right: 42rpx;
}
.search-box-placeholder {
  font-size: 34rpx;
  color: #999;
}
.tab-head {
  margin-bottom: 24rpx;
  text-align: center;
}
.no-last-tab-title {
  margin-right: 224rpx;
}
.selected-tab-title {
  font-size: 34rpx;
  color: #FECC19;
  border-bottom: 4rpx solid #FECC19; 
}
.unseleted-tab-title {
  font-size: 34rpx;
  color: #999;
}
.word-in-bottom {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #999;
  text-align: center;
}
.no-run-club {
